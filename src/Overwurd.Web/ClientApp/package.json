{
  "name": "overwurd.web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.1",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.8",
    "dayjs": "^1.10.7",
    "eslint": "7.32.0",
    "merge": "2.1.1",
    "oidc-client": "1.11.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-bootstrap": "0.25.0",
    "react-router-dom": "6.0.2",
    "react-scripts": "4.0.3",
    "rimraf": "3.0.2"
  },
  "devDependencies": {
    "@types/jest": "27.0.3",
    "@types/node": "16.11.10",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "ajv": "8.8.2",
    "cross-env": "7.0.3",
    "nan": "2.15.0",
    "typescript": "4.5.2"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-trailing-spaces": "error",
      "indent": [
        "error",
        4
      ],
      "quotes": [
        "error",
        "single"
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "no-unused-vars": "error"
    }
  },
  "scripts": {
    "start": "rimraf ./build && react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
